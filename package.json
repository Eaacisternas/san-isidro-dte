{
  "name": "example-hexagonal-template",
  "version": "0.0.0",
  "author": "San-isidro",
  "description": "Template crafted as an example of hexagonal architecture in NestJS.",
  "license": "ISC",
  "scripts": {
    "build": "nest build",
    "format": "prettier --check --ignore-unknown --write",
    "format:all": "npm run format -- .",
    "lint": "eslint --fix",
    "lint:all": "npm run lint -- \"{src,test}/**/*.ts\"",
    "lint:types": "tsc --noEmit",
    "prebuild": "rimraf app",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node app/main",
    "test": "jest --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:cov": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "up-sl-off": "cp deployment/serverless.dev.yml serverless.yml && npx serverless offline"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "@vendia/serverless-express": "^4.10.4",
    "aws-lambda": "^1.0.7",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "ts-case-convert": "^2.0.2"
  },
  "devDependencies": {
    "@automock/jest": "^1.0.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/testing": "^9.4.2",
    "@types/aws-lambda": "^8.10.116",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "husky": "^8.0.0",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "serverless": "^3.32.2",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-log-retention": "2.0.0",
    "serverless-s3-encryption": "^1.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8.15.0"
  }
}
